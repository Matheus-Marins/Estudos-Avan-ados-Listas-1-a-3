#include<stdio.h>
#include<math.h>
void main ()

{
    int A;

    printf ("Digite o valor: ");
    scanf ("%d", &A);

    if (A<3)
    {
        printf ("O valor %d e menor que 3", A);
    }

    else if (A==3)
    {
        printf ("O valor e %d", A);
    }

    else
    {
        printf ("O valor %d e maior que 3", A);
    }
}
